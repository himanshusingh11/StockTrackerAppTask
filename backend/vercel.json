{
    "version": 2,
    "builds": [
      {
        "src": "*.js", 
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)", 
        "dest": "/" ,
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "https://stock-tracker-app-task.vercel.app" }
        ] 
      }
      
    ]
  }
  